<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" maxWidth="1.7976931348623157E308">
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
         <children>
            <Label maxWidth="1.7976931348623157E308" text="Select row(s) want to import" />
         </children>
      </HBox>
      <HBox alignment="BOTTOM_RIGHT" maxWidth="1.7976931348623157E308">
         <children>
            <CheckBox fx:id="selectAllCheckBox" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Sellect all" />
         </children>
      </HBox>
      <ScrollPane fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <content>
            <TableView fx:id="importFileChamCongTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity">
               <columns>
                  <TableColumn fx:id="importFileChamCongIDColumn" editable="false" maxWidth="9999.0" minWidth="-1.0" prefWidth="-1.0" style="-fx-alignment: center;" text="ID" />
                  <TableColumn fx:id="importFileChamCongTimestampColumn" editable="false" maxWidth="9999.0" minWidth="-1.0" prefWidth="-1.0" style="-fx-alignment: center;" text="Timestamp" />
                  <TableColumn fx:id="importFileChamCongDeviceColumn" editable="false" maxWidth="9999.0" minWidth="-1.0" prefWidth="-1.0" style="-fx-alignment: center;" text="Device" />
                  <TableColumn fx:id="importFileChamCongSelectColumn" maxWidth="9999.0" minWidth="-1.0" prefWidth="-1.0" style="-fx-alignment: center;" text="Select" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </content>
      </ScrollPane>
      <HBox alignment="TOP_RIGHT" maxWidth="1.7976931348623157E308" spacing="10.0">
         <children>
            <Button fx:id="chooseAnotherFileButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Choose another file" />
            <Button fx:id="importButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Import selected" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</VBox>
